model RefreshToken {
  id           String      @id @default(uuid()) @db.UniqueIdentifier
  token        String      @unique
  userId       String      @db.VarChar(10)
  expiresAt    DateTime
  createdAt    DateTime    @default(now())
  tblUser      tblUsers    @relation(fields: [userId], references: [userid], onDelete: Cascade)

  @@index([userId])
}
    