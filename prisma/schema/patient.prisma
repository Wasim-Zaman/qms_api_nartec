model Patient {
    id              String @id @default(uuid())
    name            String?
    nationality     String?
    sex             String?
    idNumber        String?
    age             Int?
    mobileNumber    String?
    status          String?
    cheifComplaint  String?
    userId          String?
    tblUsers        tblUsers? @relation(fields: [userId], references: [userid], onDelete: Cascade)
    createdAt       DateTime @default(now())
    updatedAt       DateTime @updatedAt
}
